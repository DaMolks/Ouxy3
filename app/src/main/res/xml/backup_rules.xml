<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!-- Base de données -->
    <include domain="database" path="."/>
    
    <!-- Préférences -->
    <include domain="sharedpref" path="ouxy_secure_prefs.xml"/>
    
    <!-- Fichiers -->
    <include domain="file" path="signatures/"/>
    
    <!-- Exclusions -->
    <exclude domain="sharedpref" path="WebView.xml"/>
    <exclude domain="file" path="temp/"/>
    <exclude domain="file" path="cache/"/>
</full-backup-content>